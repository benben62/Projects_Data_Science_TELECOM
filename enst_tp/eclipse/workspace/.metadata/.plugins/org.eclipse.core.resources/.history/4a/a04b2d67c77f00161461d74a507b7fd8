package test;

public class ThreadTest extends Thread{
	int i=0;
	public ThreadTest(int id){
		i=id;
	}
	
	public void run()
	{
		if(i==1){
			out1();
		}else if(i==2){
			out2();
			
		}else if(i==3){
			out3();
		}
		else {
			System.out.println("error!");
		}
	}
	public synchronized void out1(){
		System.out.println("This is Thread "+ i);
	}
	public synchronized void out2(){
		System.out.println("This is Thread "+ i);
	}
	public synchronized void out3(){
		System.out.println("This is Thread "+ i);
	}
}
